{
  "name": "price-feed-ui",
  "version": "1.0.0",
  "description": "Price Feed UI",
  "main": "src/index.tsx",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist .papi",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "gen-shibuya-rpc": "yarn papi add -w wss://rpc.shibuya.astar.network shibuya",
    "gen-pah-rpc": "yarn papi add -w wss://testnet-passet-hub.polkadot.io pah",
    "gen-ink-v5-contract-metadata": "yarn papi ink add metadata/price_feed_consumer_ink_v5.json",
    "gen-ink-v6-contract-metadata": "yarn papi ink add metadata/price_feed_consumer_ink_v6.json",
    "generate": "yarn gen-ink-v5-contract-metadata && yarn gen-ink-v6-contract-metadata && yarn gen-shibuya-rpc && yarn gen-pah-rpc",
    "postinstall": "yarn generate"
  },
  "peerDependencies": {
    "polkadot-api": "*"
  },
  "dependencies": {
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "latest",
    "@polkadot-api/sdk-ink": "^0.4.3",
    "@polkadot-api/substrate-bindings": "^0.16.3",
    "@polkadot/api": "^16.4.8",
    "@polkadot/util": "^13.5.6",
    "@polkadot/util-crypto": "^13.5.6",
    "@reactive-dot/react": "^0.49.0",
    "dayjs": "^1.11.18",
    "polkadot-api": "^1.18.1",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "latest",
    "vite": "latest"
  }
}
